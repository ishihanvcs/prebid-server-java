        },
        "bidder": {
          "$ref": "#/definitions/imp_ext_prebid_bidder"
        }
      }
    },
    "imp_ext_prebid_improvedigitalpbs": {
      "type": "object",
      "required": [
        "accountId"
      ],
      "properties": {
        "accountId": {
          "type": "string"
        },
        "requestId": {
          "type": "string"
        },
        "bidFloors": {
          "type": "object",
          "required": [
            "default"
          ],
          "minProperties": 1,
          "additionalProperties": false,
          "patternProperties": {
            "^[a-z]{2}$": {
              "$ref": "#/definitions/imp_ext_prebid_improvedigitalpbs_bid_floor"
            }
          },
          "properties": {
            "default": {
              "$ref": "#/definitions/imp_ext_prebid_improvedigitalpbs_bid_floor"
            }
          }
        },
        "waterfall": {
          "type": "object",
          "required": [
            "default"
          ],
          "minProperties": 1,
          "additionalProperties": false,
          "patternProperties": {
            "^[a-z]{2}$": {
