  "definitions": {
    "imp_ext": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "is_rewarded_inventory": {
          "type": "boolean"
        },
        "exchangepkey": {
          "type": "string"
        },
        "exchangetagid": {
          "type": "string"
        },
        "partner_id": {
          "type": "string"
        },
        "prebid": {
          "$ref": "#/definitions/imp_ext_prebid"
        }
      }
    },
    "imp_ext_prebid": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "is_rewarded_inventory": {
